{
    "approvalRules": [
      {
        "condition": "totalAmount < 10000",
        "action": "autoApprove",
        "setStatus": "Approved"
      },
      {
        "condition": "deliveryDays < 3",
        "action": "setUrgency",
        "urgency": "High"
      },
      {
        "condition": "deliveryDays >= 3 and deliveryDays <= 7",
        "action": "setUrgency",
        "urgency": "Medium"
      },
      {
        "condition": "deliveryDays > 7",
        "action": "setUrgency",
        "urgency": "Low"
      }
    ]
  }
  